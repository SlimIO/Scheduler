{
  "name": "@slimio/scheduler",
  "version": "0.6.0",
  "description": "Slim.IO - Addon callbacks scheduler",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env npm test && eslint src/scheduler.class.js",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SlimIO/Scheduler.git"
  },
  "keywords": [
    "Addon",
    "Callbacks",
    "Scheduler"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Scheduler/issues"
  },
  "homepage": "https://github.com/SlimIO/Scheduler#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^3.0.2",
    "@slimio/is": "^1.5.0",
    "ava": "^1.4.1",
    "cross-env": "^5.2.0",
    "driftless": "^2.0.3",
    "eslint": "^5.16.0",
    "husky": "^2.3.0",
    "jsdoc": "^3.6.2",
    "nyc": "^14.1.1",
    "pkg-ok": "^2.3.1"
  }
}
